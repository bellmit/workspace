package org.jumao.bi.utis.constants;

/**
 */
public class EnJrtUrl extends BasicUrl {

    /**
     * e.g. /v1/enjrt/
     */
    private static final String V1_Enjrt = Slash + V1 + Slash + En + Jrt + Slash;

    //---------------------------------------
    //-----------   以下融资   ---------------
    //---------------------------------------

    /**
     * e.g. /v1/enjrt/financing
     */
    public static final String V1_Enjrt_Financing = V1_Enjrt + Key.Financing;


    /**
     * e.g. /trend/lineChart/type/0
     */
    public static final String Financing_Trend_LineChart = Slash + Key.Trend + Slash + Line_Chart + Tail_Type_Parm;


    /**
     * e.g. /trend/analysis/lineChart/platform/101501/startDate/20170101/endDate/20170707/type/0
     */
    public static final String Trend_Analysis_LineChart = Slash + Key.Trend + Slash + Key.Analysis + Slash + Line_Chart +
            Platform_And_Date_Range_Path_Param + Tail_Type_Parm;


    /**
     * e.g. /areaDist/global/type/0
     */
    public static final String Global_Area_Dist = Slash + Area_Dist + Slash + Global + Tail_Type_Parm;


    /**
     * e.g. /areaDist/global/platform/101501/startDate/20170101/endDate/20170707/type/0
     */
    public static final String Global_Area_Dist_Date_Range = Slash + Area_Dist + Slash + Global +
            Platform_And_Date_Range_Path_Param + Tail_Type_Parm;

    /**
     * e.g. /area/1/detail/platform/101501/startDate/20170101/endDate/20170707/limit/10/offset/0/type/0
     */
    public static final String Area_Detail_Table = Slash + Key.Area + Slash_Brace + Key.Area + Brace_Slash + Key.Detail +
            Platform_And_Date_Range_Path_Param + Limit_And_Offset_Param + Tail_Type_Parm;



}

